programa {
  funcao inicio() 
  {
    cadeia nome
    inteiro idade
    inteiro idade5a = 5
    real resultado

    escreva("Qual seu nome? ")
    leia(nome)

    escreva("Qual a sua idade? ")
    leia(idade)

    resultado = idade5a+idade

    escreva("Seu nome é: ", nome, "\n")
    escreva("Hoje tem: ",idade , " anos de idade", "\n")
    escreva("Daqui 5 anos terá: ", resultado, "\n")

  }
}
