programa {
  funcao inicio() 
  {
    real anoAtual, anoNasc, idade

    escreva("Em que ano estamos? ")
    leia(anoAtual)
 
    escreva("Em que ano você nasceu? ")
    leia(anoNasc)

    idade = anoAtual-anoNasc

    escreva("Com base nos dados aprensentados você tem: ", idade, " anos de idade")


  }
}
